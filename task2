#include <iostream>
#include <fstream>
#include <stdlib.h>

int main() {

	const int n = 10;
	int arr[n];

	int sum = 0;

	srand(2547);

	for (int i = 0; i < n; i++) {
		arr[i] = rand() % 30 - 10;
		std::cout << arr[i] << std::endl;

	}

	for (int i = 0; i < n; i++) {

		if (arr[i] > 5) {

			sum += arr[i];

		}

	}

	std::cout << "Sum is " << sum << std::endl;

	std::cin.get();
	std::cin.get();


	return 0;
}
